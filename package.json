{
  "name": "spotifyradio",
  "version": "1.0.0",
  "description": "Seja bem vindo(a) à sexta Semana Javascript Expert. Este é o código inicial para iniciar nossa jornada.",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "start": "node server/index.js",
    "live-reload": "nodemon server/index.js",
    "test": "set NODE_OPTIONS=--experimental-vm-modules && npx jest --force-exit --detectOpenHandles --runInBand --no-warnings",
    "test:watch": "set NODE_OPTIONS=--experimental-vm-modules && npx jest --force-exit --detectOpenHandles --runInBand --no-warnings --watchAll",
    "test:coverage": "set NODE_OPTIONS=--experimental-vm-modules && npx jest --force-exit --detectOpenHandles --runInBand --no-warnings --coverage tests/unit",
    "test:e2e": " set NODE_OPTIONS=--experimental-vm-modules && npx jest --force-exit --detectOpenHandles --runInBand --no-warnings --no-cache tests/e2e",
    "live-reload:docker": "docker-compose up --build live-reload",
    "test:docker": "docker-compose up --build test",
    "test:watch:docker": "docker-compose up --build test-watch",
    "test:coverage:docker": "docker-compose up --build test-coverage"
  },
  "engines": {
    "node": "18"
  },
  "keywords": [],
  "author": "Everton Jackson",
  "license": "ISC",
  "devDependencies": {
    "jest": "^27.5.1",
    "nodemon": "^2.0.15",
    "portfinder": "^1.0.28",
    "supertest": "^6.2.2"
  },
  "dependencies": {
    "i": "^0.3.7",
    "npm": "^8.7.0",
    "pino": "^7.8.0",
    "pino-pretty": "^7.5.3",
    "sox": "^0.1.0",
    "sox-stream": "^2.0.6",
    "throttle": "^1.0.3"
  }
}
