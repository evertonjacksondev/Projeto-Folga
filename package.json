{
  "name": "projetx",
  "version": "1.0.0",
  "description": "Seja bem vindo(a) à sexta Semana Javascript Expert. Este é o código inicial para iniciar nossa jornada.",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "start": "node server/index.js",
    "live-reload": "nodemon server/index.js",
    "test": "set NODE_OPTIONS=--experimental-vm-modules && npx jest --detectOpenHandles --runInBand",
    "test:watch": "set NODE_OPTIONS='--experimental-vm-modules --no-warnings' jest --no-cache --watchAll",
    "test:coverage": "set NODE_OPTIONS=--experimental-vm-modules && npx jest --detectOpenHandles --runInBand --coverage tests/unit",
    "live-reload:docker": "docker-compose up live-reload"
  },
  "engines": {
    "node": "18"
  },
  "keywords": [],
  "author": "dev.EvertonJackson",
  "license": "ISC",
  "devDependencies": {
    "jest": "^27.5.1",
    "nodemon": "^2.0.15",
    "portfinder": "^1.0.28",
    "supertest": "^6.2.2"
  },
  "dependencies": {
    "pino": "^7.8.0",
    "pino-pretty": "^7.5.3",
    "sox": "^0.1.0",
    "throttle": "^1.0.3"
  }
}
